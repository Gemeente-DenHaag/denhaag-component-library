{
  "denhaag": {
    "cta-image-content": {
      "background-color": {
        "value": "{denhaag.color.white}"
      },
      "border": {
        "value": "1px solid {denhaag.color.grey.2}"
      },
      "display": {
        "value": "flex"
      },
      "flex-direction": {
        "value": "column-reverse"
      },
      "image": {
        "aspect-ratio": {
          "value": "16 / 9"
        },
        "object-fit": {
          "value": "cover"
        },
        "width": {
          "value": "100%"
        },
        "height": {
          "value": "100%"
        }
      },
      "image-container": {
        "width": {
          "value": "100%"
        },
        "overflow": {
          "value": "hidden"
        }
      },
      "content": {
        "padding-inline": {
          "value": "{denhaag.space.inline.2xl}"
        },
        "padding-block": {
          "value": "{denhaag.space.block.2xl}"
        },
        "width": {
          "value": "calc(50% - (2 * {denhaag.space.inline.2xl}))"
        }
      },
      "heading": {
        "font-size": {
          "value": "{denhaag.typography.scale.xl.font-size}"
        },
        "line-height": {
          "value": "{denhaag.typography.scale.xl.line-height}"
        }
      },
      "paragraph": {
        "font-size": {
          "value": "{denhaag.typography.scale.lg.font-size}"
        },
        "line-height": {
          "value": "{denhaag.typography.scale.lg.line-height}"
        }
      },
      "translate": {
        "value": "translateY(-0.125rem)"
      },
      "transition": {
        "duration": {
          "value": "200ms"
        },
        "timing-function": {
          "value": "ease-in-out"
        }
      },
      "illustration-variant": {
        "content": {
          "padding-inline-end": {
            "value": "4.5rem"
          },
          "padding-inline-start": {
            "value": "3.5rem"
          }
        },
        "image-container": {
          "margin": {
            "value": "{denhaag.space.block.2xl}"
          },
          "width": {
            "value": "auto"
          }
        }
      }
    }
  }
}
