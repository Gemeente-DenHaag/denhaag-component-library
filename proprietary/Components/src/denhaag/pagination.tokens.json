{
  "denhaag": {
    "pagination": {
      "border": {
        "radius": {
          "value": "{denhaag.border-radius.value}"
        }
      },
      "font": {
        "size": {
          "value": "16px"
        },
        "weight": {
          "value": "500"
        }
      },
      "margin-inline": {
        "value": "10px"
      },
      "transition": {
        "ease": {
          "value": "cubic-bezier(0.55, 0, 0.1, 1)"
        },
        "speed": {
          "value": "375ms"
        }
      },
      "size": {
        "value": "32px"
      },
      "arrow": {
        "font-size": {
          "value": "12px"
        },
        "size": {
          "value": "20px"
        }
      },
      "link": {
        "background-color": {
          "value": "transparent"
        },
        "color": {
          "value": "{denhaag.color.grey.4.value}"
        },
        "hover": {
          "color": {
            "value": "{denhaag.color.green.3.value}"
          }
        },
        "disabled": {
          "color": {
            "value": "{denhaag.color.grey.2.value}"
          }
        },
        "current": {
          "background-color": {
            "value": "{denhaag.color.green.3.value}"
          },
          "color": {
            "value": "{denhaag.color.white.value}"
          },
          "hover": {
            "background-color": {
              "value": "{denhaag.color.green.4.value}"
            },
            "color": {
              "value": "{denhaag.color.white.value}"
            }
          },
          "disabled": {
            "background-color": {
              "value": "{denhaag.color.grey.2.value}"
            },
            "color": {
              "value": "{denhaag.color.white.value}"
            }
          }
        }
      }
    }
  }
}
