{
  "name": "@gemeente-denhaag/design-system",
  "version": "0.1.0",
  "private": true,
  "description": "A library of React components for the Design System of The Netherlands",
  "bugs": "https://github.com/nl-design-system/denhaag/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/nl-design-system/denhaag.git"
  },
  "license": "EUPL-1.2",
  "author": "Municipality of The Hague",
  "workspaces": [
    "packages/*",
    "components/*",
    "proprietary/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "build-storybook": "build-storybook -s .storybook/stories/assets/",
    "clean": "lerna run clean --parallel && rimraf .rollup-cache",
    "lint": "npm-run-all --continue-on-error lint:* && lerna run lint",
    "lint-fix": "npm-run-all --continue-on-error lint-fix:* prettier",
    "lint-fix:css": "stylelint --fix {components,packages,properietary}/**/*.css",
    "lint-fix:js": "eslint --fix '*/**/*.{js,jsx,ts,tsx}'",
    "lint:css": "stylelint {components,packages,properietary}/**/*.css",
    "lint:package": "npmPkgJsonLint */**/package.json",
    "lint:js": "eslint '*/**/*.{js,jsx,ts,tsx}'",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --write .",
    "prettier-check": "prettier -c */**/*.{js,json,jsx,ts,tsx}",
    "publish": "lerna publish from-package",
    "release": "lerna version prerelease --no-changelog --no-private --no-commit-hooks",
    "storybook": "npm-run-all --parallel watch:*",
    "test": "cypress run-ct",
    "test:open": "cypress open-ct",
    "update-patch": "npm-check-updates --configFileName .ncurc.patch.js --deep --dep dev,prod --target patch --upgrade && npm install --legacy-peer-deps",
    "update-minor": "npm-check-updates --configFileName .ncurc.minor.js --deep --dep dev,prod --target minor --upgrade && npm install --legacy-peer-deps",
    "update-major": "npm-check-updates --configFileName .ncurc.major.js --deep --dep dev,prod --target latest --upgrade && npm install --legacy-peer-deps",
    "watch:storybook": "start-storybook -p 6006 -s .storybook/stories/assets/",
    "watch:style-dictionary": "lerna run --parallel --scope '@gemeente-denhaag/design-tokens-*' watch:style-dictionary",
    "typecheck:storybook": "tsc --project tsconfig.storybook.json --noEmit"
  },
  "dependencies": {
    "@material-ui/core": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@material-ui/pickers": "3.2.10",
    "@utrecht/component-library-css": "1.0.0-alpha.107",
    "clsx": "1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@cypress/react": "5.4.2",
    "@cypress/webpack-dev-server": "1.1.6",
    "@etchteam/storybook-addon-status": "4.0.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-typescript": "8.2.1",
    "@storybook/addon-a11y": "6.3.2",
    "@storybook/addon-actions": "6.3.2",
    "@storybook/addon-essentials": "6.3.2",
    "@storybook/addon-links": "6.3.2",
    "@storybook/addons": "6.3.2",
    "@storybook/react": "6.3.2",
    "@svgr/rollup": "5.5.0",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.1.9",
    "@types/jest": "27.0.1",
    "@types/node": "16.3.2",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "axe-core": "4.2.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "27.0.2",
    "babel-loader": "8.1.0",
    "chokidar-cli": "3.0.0",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.2",
    "cypress": "8.0.0",
    "cypress-axe": "0.13.0",
    "cypress-plugin-snapshots": "1.4.4",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lerna": "4.0.0",
    "lint-staged": "12.0.2",
    "lodash": "4.17.21",
    "mdx-embed": "0.0.22",
    "npm-check-updates": "12.0.2",
    "npm-package-json-lint": "5.1.0",
    "npm-run-all": "4.1.5",
    "pinst": "2.1.1",
    "postcss": "8.3.5",
    "prettier": "2.2.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-is": "17.0.1",
    "react-scripts": "4.0.3",
    "react-test-renderer": "17.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.50.6",
    "rollup-plugin-import-css": "3.0.0",
    "rollup-plugin-postcss": "4.0.0",
    "rollup-plugin-svgo": "1.1.0",
    "storybook": "6.3.2",
    "storybook-addon-themes": "6.1.0",
    "storybook-design-token": "1.1.1",
    "style-dictionary": "3.0.1",
    "stylelint": "13.8.0",
    "stylelint-config-prettier": "9.0.3",
    "typescript": "4.1.3",
    "webpack-dev-server": "3.11.2"
  },
  "engines": {
    "node": "^16.13.0",
    "npm": "^8"
  }
}
