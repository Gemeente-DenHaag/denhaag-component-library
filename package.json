    "@gemeente-denhaag/design-tokens-components": "workspace:*",
    "@gemeente-denhaag/divider": "workspace:*",
    "@gemeente-denhaag/form-field": "workspace:*",
    "@gemeente-denhaag/form-progress": "workspace:*",
    "@gemeente-denhaag/formcontrol": "workspace:*",
    "@gemeente-denhaag/formcontrollabel": "workspace:*",
    "@gemeente-denhaag/formgroup": "workspace:*",
    "@gemeente-denhaag/iconbutton": "workspace:*",
    "@gemeente-denhaag/icons": "workspace:*",
    "@gemeente-denhaag/inputlabel": "workspace:*",
    "@gemeente-denhaag/link": "workspace:*",
    "@gemeente-denhaag/list": "workspace:*",
    "@gemeente-denhaag/menu": "workspace:*",
    "@gemeente-denhaag/posttypelabel": "workspace:*",
    "@gemeente-denhaag/process-steps": "workspace:*",
    "@gemeente-denhaag/radio": "workspace:*",
    "@gemeente-denhaag/select": "workspace:*",
    "@gemeente-denhaag/sidenav": "workspace:*",
    "@gemeente-denhaag/stylesprovider": "workspace:*",
