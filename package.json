{
  "name": "@gemeente-denhaag/design-system",
  "version": "0.1.0",
  "private": true,
  "description": "A library of React components for the Design System of The Netherlands",
  "bugs": "https://github.com/nl-design-system/denhaag/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/nl-design-system/denhaag.git"
  },
  "license": "EUPL-1.2",
  "author": "Municipality of The Hague",
  "workspaces": [
    "packages/*",
    "components/*",
    "proprietary/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "build-storybook": "build-storybook -s .storybook/stories/assets/",
    "clean": "lerna run clean --parallel && rimraf .rollup-cache",
    "lint": "npm-run-all --continue-on-error lint:** && lerna run lint",
    "lint-fix": "npm-run-all --continue-on-error lint-fix:** prettier",
    "lint-fix:css": "stylelint --fix '**/*.{css,scss}'",
    "lint-fix:js": "eslint --ext '.js,.json,.jsx,.mdx,.ts,.tsx' --fix --report-unused-disable-directives .",
    "lint:css": "stylelint '**/*.{css,scss}'",
    "lint:js": "eslint --ext '.js,.json,.jsx,.mdx,.ts,.tsx' --report-unused-disable-directives .",
    "lint:package": "npmPkgJsonLint '**/package.json'",
    "lint:prettier": "prettier --check .",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --write .",
    "publish": "lerna publish from-package",
    "release": "lerna version prerelease --no-changelog --no-private --no-commit-hooks",
    "storybook": "npm-run-all --parallel watch:*",
    "test": "cypress run-ct",
    "test:open": "cypress open-ct",
    "update-patch": "npm-check-updates --configFileName .ncurc.patch.js --deep --dep dev,prod --packageManager yarn --target patch --upgrade && yarn install",
    "update-minor": "npm-check-updates --configFileName .ncurc.minor.js --deep --dep dev,prod --packageManager yarn --target minor --upgrade && yarn install",
    "update-major": "npm-check-updates --configFileName .ncurc.major.js --deep --dep dev,prod --packageManager yarn --target latest --upgrade && yarn install",
    "watch:storybook": "start-storybook -p 6006 -s .storybook/stories/assets/",
    "watch:style-dictionary": "lerna run --parallel --scope '@gemeente-denhaag/design-tokens-*' watch:style-dictionary",
    "typecheck:storybook": "yarn tsc --project tsconfig.storybook.json --noEmit"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/lab": "4.11.3-deprecations.1",
    "@material-ui/pickers": "3.3.10",
    "@utrecht/component-library-css": "1.0.0-alpha.157",
    "clsx": "1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@babel/preset-env": "7.16.5",
    "@babel/preset-react": "7.16.5",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@cypress/react": "5.12.0",
    "@cypress/webpack-dev-server": "1.8.0",
    "@etchteam/storybook-addon-status": "4.1.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.1",
    "@rollup/plugin-typescript": "8.3.0",
    "@storybook/addon-a11y": "6.4.9",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/addon-links": "6.4.9",
    "@storybook/addons": "6.4.9",
    "@storybook/react": "6.4.9",
    "@svgr/rollup": "5.5.0",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.17",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "axe-core": "4.3.5",
    "babel-eslint": "10.0.3",
    "babel-jest": "27.4.5",
    "babel-loader": "8.2.3",
    "chokidar-cli": "3.0.0",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "cypress": "8.7.0",
    "cypress-axe": "0.13.0",
    "cypress-plugin-snapshots": "1.4.4",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-mdx": "1.16.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "lerna": "4.0.0",
    "lint-staged": "12.1.3",
    "lodash": "4.17.21",
    "mdx-embed": "0.0.22",
    "npm-check-updates": "12.0.5",
    "npm-package-json-lint": "5.4.2",
    "npm-run-all": "4.1.5",
    "pinst": "2.1.6",
    "postcss": "8.4.5",
    "prettier": "2.5.1",
    "prop-types": "15.8.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-scripts": "4.0.3",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.61.1",
    "rollup-plugin-import-css": "3.0.2",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-svgo": "1.1.0",
    "storybook": "6.4.9",
    "storybook-addon-themes": "6.1.0",
    "storybook-design-token": "1.4.0",
    "style-dictionary": "3.1.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "9.0.3",
    "typescript": "4.5.4",
    "webpack-dev-server": "3.11.3",
    "yarn": "1.22.17"
  },
  "engines": {
    "node": "^16.13.0",
    "yarn": "^1.22.0"
  }
}
