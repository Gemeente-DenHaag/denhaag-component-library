{
  "name": "@gemeente-denhaag/design-system",
  "version": "0.1.0",
  "private": true,
  "description": "A library of React components for the Design System of The Netherlands",
  "bugs": "https://github.com/nl-design-system/denhaag/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/nl-design-system/denhaag.git"
  },
  "license": "EUPL-1.2",
  "author": "Municipality of The Hague",
  "workspaces": [
    "packages/*",
    "components/*",
    "proprietary/*"
  ],
  "scripts": {
    "build": "pnpm run build:library && pnpm run build:storybook",
    "build:library": "pnpm run --recursive build",
    "build:storybook": "pnpm run --if-present --recursive build:storybook",
    "clean": "pnpm run --parallel --recursive clean && rimraf .rollup-cache",
    "lint": "npm-run-all --continue-on-error lint:** && pnpm run --recursive lint",
    "lint-fix": "npm-run-all --continue-on-error lint-fix:** prettier",
    "lint-fix:css": "stylelint --fix \"**/*.{css,scss}\"",
    "lint-fix:js": "eslint --ext \".js,.json,.jsx,.mdx,.mjs,.ts,.tsx\" --fix --report-unused-disable-directives .",
    "lint:css": "stylelint \"**/*.{css,scss}\"",
    "lint:js": "eslint --ext .js,.json,.jsx,.mdx,.mjs,.ts,.tsx --report-unused-disable-directives .",
    "lint:package": "npmPkgJsonLint \"**/package.json\"",
    "lint:prettier": "prettier --check .",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --write .",
    "publish": "lerna publish from-package --yes",
    "release": "lerna version prerelease --no-changelog --no-private --no-commit-hooks --yes",
    "release-denhaag-nl": "lerna version prerelease --no-changelog --no-private --no-commit-hooks --yes --no-git-tag-version --preid www-denhaag-nl",
    "storybook": "npm-run-all --parallel watch:*",
    "test-update": "npm-run-all --sequential lint build",
    "update-patch": "npm-check-updates --configFileName .ncurc.patch.js --deep --dep dev,prod --target patch --upgrade && pnpm install",
    "update-minor": "npm-check-updates --configFileName .ncurc.minor.js --deep --dep dev,prod --target minor --upgrade && pnpm install",
    "update-major": "npm-check-updates --configFileName .ncurc.major.js --deep --dep dev,prod --target latest --upgrade && pnpm install",
    "watch:components": "pnpm run --parallel --recursive watch:components",
    "watch:storybook": "pnpm run --if-present --recursive storybook",
    "watch:style-dictionary": "pnpm run --parallel --recursive watch:style-dictionary",
    "typecheck": "pnpm run --recursive typecheck"
  },
  "dependencies": {
    "@utrecht/component-library-css": "3.0.0",
    "@utrecht/components": "3.0.0",
    "clsx": "2.1.0",
    "sass": "1.69.7"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-react": "7.23.3",
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@lerna-lite/cli": "3.2.1",
    "@lerna-lite/publish": "3.2.1",
    "@lerna-lite/version": "3.2.1",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@svgr/rollup": "8.1.0",
    "@svgr/webpack": "8.1.0",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.8",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "@utrecht/component-library-react": "3.0.0",
    "axe-core": "4.8.3",
    "babel-jest": "29.7.0",
    "babel-loader": "9.1.3",
    "chokidar-cli": "3.0.0",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "ejs": "3.1.9",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-mdx": "2.3.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "express": "4.18.2",
    "helmet": "7.1.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "14.0.1",
    "npm-check-updates": "16.14.12",
    "npm-package-json-lint": "7.1.0",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "postcss": "8.4.33",
    "postcss-discard-duplicates": "6.0.1",
    "prettier": "3.1.1",
    "raw-loader": "4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "react-test-renderer": "18.2.0",
    "rimraf": "5.0.5",
    "rollup": "3.29.4",
    "rollup-plugin-import-css": "3.4.0",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-summary": "2.0.0",
    "rollup-plugin-svgo": "2.0.0",
    "rollup-plugin-typescript2": "0.36.0",
    "sass-loader": "13.3.3",
    "style-dictionary": "3.9.1",
    "stylelint": "15.11.0",
    "stylelint-config-standard-scss": "11.1.0",
    "stylelint-order": "6.0.4",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "uuid": "9.0.1",
    "webpack": "5.89.0",
    "webpack-dev-server": "4.15.1",
    "yarn": "1.22.21"
  },
  "engines": {
    "node": "^20",
    "pnpm": "^8"
  }
}
