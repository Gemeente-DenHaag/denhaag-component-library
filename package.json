{
  "name": "@gemeente-denhaag/design-system",
  "version": "0.1.0",
  "private": true,
  "description": "A library of React components for the Design System of The Netherlands",
  "bugs": "https://github.com/nl-design-system/denhaag/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/nl-design-system/denhaag.git"
  },
  "license": "EUPL-1.2",
  "author": "Municipality of The Hague",
  "workspaces": [
    "packages/*",
    "components/*",
    "proprietary/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "yarn build:library && yarn build:storybook",
    "build:library": "lerna run build",
    "build:storybook": "build-storybook",
    "clean": "lerna run clean --parallel && rimraf .rollup-cache",
    "lint": "npm-run-all --continue-on-error lint:** && lerna run lint",
    "lint-fix": "npm-run-all --continue-on-error lint-fix:** prettier",
    "lint-fix:css": "stylelint --fix \"**/*.{css,scss}\"",
    "lint-fix:js": "eslint --ext \".js,.json,.jsx,.mdx,.ts,.tsx\" --fix --report-unused-disable-directives .",
    "lint:css": "stylelint \"**/*.{css,scss}\"",
    "lint:js": "eslint --ext .js,.json,.jsx,.mdx,.ts,.tsx --report-unused-disable-directives .",
    "lint:package": "npmPkgJsonLint \"**/package.json\"",
    "lint:prettier": "prettier --check .",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --write .",
    "publish": "lerna publish from-package --yes",
    "release": "lerna version prerelease --no-changelog --no-private --no-commit-hooks --yes",
    "release-denhaag-nl": "lerna version prerelease --no-changelog --no-private --no-commit-hooks --yes --no-git-tag-version --preid www-denhaag-nl",
    "storybook": "npm-run-all --parallel watch:*",
    "update-patch": "npm-check-updates --configFileName .ncurc.patch.js --deep --dep dev,prod --packageManager yarn --target patch --upgrade && yarn install",
    "update-minor": "npm-check-updates --configFileName .ncurc.minor.js --deep --dep dev,prod --packageManager yarn --target minor --upgrade && yarn install",
    "update-major": "npm-check-updates --configFileName .ncurc.major.js --deep --dep dev,prod --packageManager yarn --target latest --upgrade && yarn install",
    "watch:storybook": "start-storybook -p 6006",
    "watch:style-dictionary": "lerna run --parallel --scope '@gemeente-denhaag/design-tokens-*' watch:style-dictionary",
    "typecheck:storybook": "yarn tsc --project tsconfig.storybook.json --noEmit"
  },
  "dependencies": {
    "@material-ui/core": "4.12.4",
    "@material-ui/lab": "4.11.3-deprecations.1",
    "@utrecht/component-library-css": "1.0.0-alpha.498",
    "@utrecht/components": "1.0.0-alpha.459",
    "clsx": "1.2.1",
    "sass": "1.62.1"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-react": "7.18.6",
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "@etchteam/storybook-addon-status": "4.2.2",
    "@lerna-lite/cli": "2.5.0",
    "@lerna-lite/publish": "2.5.0",
    "@lerna-lite/run": "2.5.0",
    "@rollup/plugin-commonjs": "21.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-typescript": "8.5.0",
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addons": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.5.16",
    "@svgr/rollup": "5.5.0",
    "@svgr/webpack": "5.5.0",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.25",
    "@types/react": "17.0.58",
    "@types/react-dom": "17.0.20",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "axe-core": "4.7.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.5.1",
    "babel-loader": "8.3.0",
    "chokidar-cli": "3.0.0",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-mdx": "1.17.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.5.0",
    "lodash": "4.17.21",
    "mdx-embed": "0.0.23",
    "npm-check-updates": "12.5.12",
    "npm-package-json-lint": "5.4.2",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "postcss": "8.4.23",
    "postcss-discard-duplicates": "5.1.0",
    "prettier": "2.8.8",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-scripts": "4.0.3",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-import-css": "3.2.1",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-summary": "1.4.3",
    "rollup-plugin-svgo": "1.1.0",
    "sass-loader": "13.2.2",
    "storybook": "6.5.16",
    "storybook-addon-themes": "6.1.0",
    "storybook-design-token": "2.9.0",
    "style-dictionary": "3.8.0",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-order": "5.0.0",
    "typescript": "4.9.5",
    "webpack": "5.82.0",
    "webpack-dev-server": "3.11.3",
    "yarn": "1.22.19"
  },
  "engines": {
    "node": "^16 || ^18.12",
    "yarn": "^1.22.0"
  }
}
